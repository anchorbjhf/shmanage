        //#region 遍历数组，组成带","号的字符串
        /*
        Array:数组名称
        */
        function SplitArray(Array) {
            var ArrayValue = '';
            if (Array.length > 1) {
                $.each(Array, function (key, val) {
                    ArrayValue += val + ",";
                });
            } else {
                $.each(Array, function (key, val) {
                    ArrayValue += val;
                });
            }
            return ArrayValue;
        }
