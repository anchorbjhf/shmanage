        //#region 根据Index设置
        /*
        Name:控件名称
        Index:需要勾选的顺序号
        IsCheck:是否勾选
        */
        function SetCheckBox(Name, Index, IsCheck) {
            var i = 0;
            $('input[name="' + Name + '"]').each(function () {
                if (i == Index) {
                    if (IsCheck == "True") {
                        $(this).prop("checked", true);
                        return false;
                    } else {
                        $(this).prop("checked", false);
                        return false;
                    }
                }
                i++;
            });
        }
