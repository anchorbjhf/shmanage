    <div align="center" style="min-width: 700px; height:auto">
        <table style="width: 100%;height:98%" border="0" cellspacing="6" cellpadding="0" class="MainTable">
            <tr>
                <td>
                    <table style="width:100%" cellspacing="0" cellpadding="6" class="blockTable">
                        <tr>
                            <td>
                                仓库分类
                            </td>
                        </tr>
                        <tr>
                            <td style="padding:0 8px 4px" align="center">
                                <table style="width:98%" border="0" cellpadding="0" cellspacing="0">
                                    <tr>
                                        <td>
                                            <table style="width:100%" border="0" cellpadding="0" cellspacing="0">
                                                <tr>
                                                    <td align="right" valign="middle"></td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <table style="width:100%" border="0" cellpadding="0" cellspacing="0">
                                                            <tr>
                                                                <td style="width:2%" height="22px"></td>
                                                                <td style="width:35%"></td>
                                                                <td style="width:5%"></td>
                                                                <td style="width:48%"></td>
                                                                <td style="width:10%"></td>
                                                            </tr>
                                                            <tr>
                                                                <td height="380px">
                                                                    &nbsp;
                                                                </td>
                                                                <td align="center" valign="middle" style="border:solid #f10f0f">
                                                                    <div>
                                                                        <div class="easyui-panel" style="padding:5px">
                                                                            <ul class="easyui-tree" data-options="url:'tree_data1.json',method:'get',animate:true,lines:true">
                                                                                <li>仓库分类</li>

                                                                            </ul>
                                                                            <div>now time: @DateTime.Now</div>
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    &nbsp;
                                                                </td>
                                                                <td rowspan="3" align="center" valign="middle" style="border:solid #000000">
                                                                    仓库管理
                                                                </td>
                                                                <td>&nbsp;</td>
                                                            </tr>
                                                            <tr>
