@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>仓库分类</title>
    @Styles.Render("~/EasyUICSS")
    @Scripts.Render("~/EasyUIJS")
</head>
<body>
