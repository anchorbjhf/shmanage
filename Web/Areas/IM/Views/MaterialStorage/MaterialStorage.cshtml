                                                                <td>更改节点</td>
                                                                <td>&nbsp;</td>
                                                                <td>&nbsp;</td>
                                                                <td>&nbsp;</td>
                                                            </tr>
                                                            <tr>
                                                                <td height="80px">&nbsp;</td>
                                                                <td>
                                                                    <div style="margin-top: 5px">
                                                                        <a href="#" id="btnAdd" class="easyui-linkbutton" iconcls="icon-add" plain="true">添加仓库子节点</a>
                                                                        <a href="#" id="btnAdd" class="easyui-linkbutton" iconcls="icon-add" plain="true">添加仓库邻节点</a>
                                                                        <a href="#" id="btnUpdate" class="easyui-linkbutton" iconcls="icon-edit" plain="true">修改</a>
                                                                        <a href="#" id="btnDelete" class="easyui-linkbutton" iconcls="icon-remove" plain="true">删除</a>
                                                                    </div>
